package com.niit.model;

import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table
public class OrderDetail 
{
@Id
@GeneratedValue
int orderId;

int cartId;
String username;
Date orderDate;
int totalAmount;
 String shippingAddr;
 String tranType;